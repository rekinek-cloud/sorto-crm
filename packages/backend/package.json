{
  "name": "@crm-gtd/backend",
  "version": "0.1.0",
  "description": "CRM-GTD SaaS Backend API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-comprehensive": "tsx prisma/seed-comprehensive.ts",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "db:setup-comprehensive": "npm run db:generate && npm run db:push && npm run db:seed-comprehensive",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.0",
    "@slack/web-api": "^7.9.2",
    "aws-sdk": "^2.1455.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-winston": "^4.2.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "imap": "^0.8.19",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.3",
    "multer": "^1.4.5-lts.1",
    "node-pty": "^1.1.0",
    "nodemailer": "^6.10.1",
    "openai": "^4.28.0",
    "rate-limit-redis": "^4.3.1",
    "redis": "^4.6.8",
    "sharp": "^0.32.5",
    "simple-imap": "^0.1.6",
    "stripe": "^13.5.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.19.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.18.1",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prisma": "^6.19.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.2"
  }
}
